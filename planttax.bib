  shorttitle = {Scale-Specific Habitat Relationships Influence Patch Occupancy},
  author = {Guttery, Michael R. and Ribic, Christine A. and Sample, David W. and Paulios, Andy and Trosen, Chris and Dadisman, John and Schneider, Daniel and Horton, Josephine A.},
  year = {2017},
