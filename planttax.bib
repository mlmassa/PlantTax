@article{irvin2013,
  title = {A Multi-Scale Occupancy Model for the Grasshopper Sparrow in the {{Mid-Atlantic}}},
  author = {Irvin, Eric and Duren, Kenneth R. and Buler, Jeffrey J. and Jones, William and Gonzon, Anthony T. and Williams, Christopher K.},
