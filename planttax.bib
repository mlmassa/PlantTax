}

@article{cabrera-cruz2020,
  title = {Artificial {{Light}} at {{Night}} Is {{Related}} to {{Broad-Scale Stopover Distributions}} of {{Nocturnally Migrating Landbirds}} along the {{Yucatan Peninsula}}, {{Mexico}}},
