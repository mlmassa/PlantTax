@article{spatz2014,
  title = {{The Biogeography of Globally Threatened Seabirds and Island Conservation Opportunities}},
  author = {Spatz, Dena R. and Newton, Kelly M. and Heinz, Reina and Tershy, Bernie and Holmes, Nick D. and Butchart, Stuart H. M. and Croll, Donald A.},
