  author = {Zimmerman, John L.},
  year = {1992},
  journal = {Wilson Bulletin},
  volume = {104},
  number = {1},
  pages = {85--94},
  publisher = {{Wilson Ornithological Society}},
  issn = {0043-5643},
  abstract = {The low species richness in grassland bird communities has been hypothesized to result from competition in a simplistic, saturated habitat (Cody) or to result from the periodic occurrence of climatic extremes that prevent drought-susceptible species from perennial settlement (Wiens). Predictions based on these two hypotheses were evaluated using June population estimates on burned and unburned tallgrass prairie over the decade, 1981-1990, on the Konza Prairie Research Natural Area, Kansas. The results of these analyses support the notion that drought tolerance is the primary factor that determines membership in the tallgrass prairie community. When drought and fire are coincidental, however, the community does respond in a manner commensurate with the saturation hypothesis by a reduction in bird abundances but not a decrease in species richness.},
  file = {C\:\\Users\\megan\\Zotero\\storage\\Y8HAKGZ4\\Zimmerman - 1992 - Density-Independent Factors Affecting the Avian Di.pdf}
}

