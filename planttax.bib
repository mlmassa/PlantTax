@article{kelly1999,
  title = {Influence of {{Summer Biogeography}} on {{Wood Warbler Stopover Abundance}}},
  author = {Kelly, Jeffrey F. and Smith, Rob and Finch, Deborah M. and Moore, Frank R. and Yong, Wang},
  year = {1999},
