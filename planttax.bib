@article{kiss2006,
  title = {High Resilience of {{Mediterranean}} Land Snail Communities to Wildfires},
  author = {Kiss, Laurence and Magnin, Frédéric},
  year = {2006},
  journal = {Biodiversity and Conservation},
  volume = {15},
  number = {9},
